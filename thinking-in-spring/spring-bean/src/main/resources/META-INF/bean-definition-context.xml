<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

       <!--导入dependency-lookup-context.xml from ioc-container-overview-->
       <import resource="classpath:/META-INF/dependency-lookup-context.xml"></import>

       <!--spring ioc container user 关联 xiaomage-user-->
       <alias name="user" alias="xiaomage-user"></alias>
</beans>